{
    "configurations": [
        {
            "name": "BitmansESP-IDF",
            "compilerPath": "${env:IDF_TOOLS_PATH}/tools/xtensa-esp-elf/esp-13.2.0_20240530/xtensa-esp-elf/bin/xtensa-esp32-elf-gcc.exe",            
            "cStandard": "c11",
            "cppStandard": "c++17",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:IDF_PATH}/components/**",
                "${env:IDF_PATH}/components/*/*/include",
                "${env:IDF_TOOLS_PATH}/tools/**",
                "${workspaceFolder}/components/**/include",
                "${workspaceFolder}/components/bitmans_lib/include"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${env:IDF_PATH}/components",
                    "${env:IDF_TOOLS_PATH}/tools",
                    "${workspaceFolder}/components"
                ],
                "limitSymbolsToIncludedHeaders": false
            },
            "defines": [],
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}