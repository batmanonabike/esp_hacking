{
    "configurations": [
        {
            "name": "BitmansESP-IDF",
            "compilerPath": "${BAT_ESP32_GCC}",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:IDF_PATH}/components/**",
                "${env:IDF_PATH}/components/*/*/include",
                "${env:IDF_TOOLS_PATH}/tools/**",
                "${workspaceFolder}/components/**/include",
                "${workspaceFolder}/components/bat_lib/include",
                "${workspaceFolder}/components/bat_ble_lib/include",
                "${workspaceFolder}/components/bat_config/include"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${env:IDF_PATH}/components",
                    "${env:IDF_TOOLS_PATH}/tools",
                    "${workspaceFolder}/components"
                ],
                "limitSymbolsToIncludedHeaders": false
            },
            "defines": [],
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}